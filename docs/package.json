{
  "name": "svelte-tweakpane-ui-docs",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "astro": "astro",
    "build": "run-s --print-label build:*",
    "build:0-astro": "pnpm run clean && astro check && astro build",
    "build:1-add-link-prefix": "tsx ./scripts/add-link-prefix.ts",
    "build:2-strip-link-suffix": "tsx ./scripts/strip-link-suffix.ts",
    "build:3-trailing-slash-fix": "bash ./scripts/migrate-index.sh",
    "clean": "rimraf .astro dist",
    "dev": "astro dev",
    "generate-acknowledgments-data": "mkdir -p ./src/content/acknowledgments && pnpm licenses list --json > ./src/content/acknowledgments/acknowledgments-docs.json",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/starlight": "^0.13.0",
    "@astrojs/svelte": "^4.0.4",
    "@types/lodash-es": "^4.17.12",
    "astro": "^3.6.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "lodash-es": "^4.17.21",
    "marked": "^10.0.0",
    "sharp": "^0.32.6",
    "svelte": "^4.2.7",
    "svelte-tweakpane-ui": "link:..",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "glob": "^10.3.10",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-pkg": "^0.18.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.5.0"
  }
}
