{
  "name": "svelte-tweakpane-ui",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://github.com/kitschpatrol/svelte-tweakpane-ui",
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "svelte": "./src/lib/index.ts"
    }
  },
  "types": "./src/lib/index.ts",
  "files": [
    "./src/lib"
  ],
  "keywords": [
    "components",
    "ui",
    "component",
    "svelte",
    "sveltekit",
    "tweakpane",
    "component-library",
    "ui-components",
    "ui-framework",
    "components-library"
  ],
  "scripts": {
    "build": "vite build && npm run package",
    "check": "svelte-kit sync && svelte-check --ignore ./scratch --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "run-s --print-label format:*",
    "format:eslint": "eslint --fix .",
    "format:prettier": "prettier --plugin prettier-plugin-svelte --write .",
    "generate-exports": "svelte-lib-helpers exports",
    "lint": "run-p --print-label lint:*",
    "lint:eslint": "eslint .",
    "lint:markdown": "markdownlint **/*.{md,mdx} --ignore node_modules",
    "lint:prettier": "prettier --plugin prettier-plugin-svelte --check .",
    "lint:typescript": "tsc --noEmit",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "svelte-local-storage-store": "^0.6.4"
  },
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-profiler": "github:kitschpatrol/tweakpane-plugin-profiler#tweakpane-v4",
    "@0b5vr/tweakpane-plugin-rotation": "github:kitschpatrol/tweakpane-plugin-rotation#tweakpane-v4",
    "@pangenerator/tweakpane-textarea-plugin": "github:kitschpatrol/tweakpane-textarea-plugin#tweakpane-v4",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.26.0",
    "@sveltejs/package": "^2.2.2",
    "@tweakpane/core": "^2.0.1",
    "@tweakpane/plugin-camerakit": "^0.3.0",
    "@tweakpane/plugin-essentials": "^0.2.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "esm-env": "^1.0.0",
    "markdownlint": "^0.31.1",
    "markdownlint-cli": "^0.37.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-pkg": "^0.18.0",
    "prettier-plugin-svelte": "^3.0.3",
    "publint": "^0.2.5",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^34.0.0",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-lib-helpers": "^0.3.15",
    "tslib": "^2.6.2",
    "tweakpane": "^4.0.1",
    "tweakpane-image-plugin": "github:kitschpatrol/tweakpane-image-plugin#tweakpane-v4",
    "tweakpane-plugin-thumbnail-list": "github:kitschpatrol/tweakpane-plugin-thumbnail-list#tweakpane-v4",
    "tweakpane-plugin-waveform": "github:kitschpatrol/tweakpane-plugin-waveform#tweakpane-v4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "svelte": "./src/lib/index.ts"
}
