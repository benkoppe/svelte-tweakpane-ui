  "scripts": {
    "build": "run-s --print-label build:*",
    "build:01-generate-exports": "bun run ./scripts/generate-exports.ts",
    "build:02-add-source-links": "bun run ./scripts/add-source-links.ts",
    "build:03-format-embedded": "npm run format:3-embedded",
    "build:04-package": "svelte-kit sync && svelte-package && publint",
    "build:05-heal-dts-comments": "bun run ./scripts/heal-dts-comments.ts",
    "build:06-strip-component-docs": "bun run ./scripts/strip-component-docs.ts",
    "build:07-format-library": "npm run format:4-library",
    "build:08-generate-doc-data": "bun run ./scripts/generate-doc-data.ts && prettier --plugin prettier-plugin-svelte --write ./src/lib-docs/generated/data",
    "build:09-generate-example-components": "bun run ./scripts/generate-example-components.ts",
    "build:10-build-docs": "vite build",
    "check": "svelte-kit sync && svelte-check --ignore ./scratch --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --ignore ./scratch --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "run-s --print-label library:5-format ",
    "format:1-eslint": "eslint --fix .",
    "format:2-prettier": "prettier --plugin prettier-plugin-svelte --write .",
    "format:3-embedded": "bun run ./scripts/format-embedded-code.ts",
    "format:4-library": "prettier --ignore-path --plugin prettier-plugin-svelte --write ./dist",
    "lint": "run-p --print-label lint:*",
    "lint:eslint": "eslint .",
    "lint:markdown": "markdownlint **/*.{md,mdx} --ignore node_modules",
    "lint:prettier": "prettier --plugin prettier-plugin-svelte --check .",
    "lint:typescript": "tsc --noEmit",
    "prepublishOnly": "run build",
    "preview": "vite preview"
  },
